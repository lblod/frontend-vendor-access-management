  <AuDataTable @content={{this.model.firstObject.canActOnBehalfOf}} @fields="Vendors"
    @noDataMessage={{t 'vendor.subject.table.not-found'}} @sort={{this.sort}} @size={{this.size}} @page={{this.page}}
    @isLoading={{isLoading}} as |f|>
    <f.content as |c|>
      <c.header>
        <th class="data-table__header-title u-hidden--mobile">{{t 'vendor.subject.table.row-1'}}</th>
        <th class="data-table__header-title u-visible--tablet-portrait-up" style="max-width: 5rem;">{{t 'vendor.subject.table.row-2'}}</th>
        <th class="data-table__header-title u-visible--tablet-portrait-up" style="max-width: 5rem;">{{t 'vendor.subject.table.row-3'}}</th>
      </c.header>
      <c.body as |bestuurseenheid|>
        <td>
          {{bestuurseenheid.naam}}
        </td>
        <td>
          {{bestuurseenheid.classificatie.label}}
        </td>
        <td>
          <AuButton @alert="true" {{ on 'click' (fn this.removeFromList bestuurseenheid this.model.firstObject)}}>
            Delete</AuButton>
        </td>
      </c.body>
    </f.content>
  </AuDataTable>